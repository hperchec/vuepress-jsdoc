{
  "name": "vuepress-jsdoc-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docs": "node ../bin/vuepress-jsdoc.js --jsDocConfigPath=jsdoc.json --docgenConfigPath=docgen.config.js --source=./src --dist=./documentation --title=API --exclude=class.js --partials=./partials/*.hbs --readme=./README.md",
    "docs:dev": "npm run docs -- --watch",
    "dev": "npm run docs && vuepress dev documentation",
    "build:local": "npm run docs && vuepress build documentation",
    "build": "vuepress build documentation"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "concurrently": "^7.4.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "react": "^18.2.0",
    "typescript": "^4.8.4",
    "vuepress": "^1.9.8"
  }
}
